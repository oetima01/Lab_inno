#include <stdio.h>
#include <string.h>

int main(void) {
    char s[3] = {' ', ' ', ' '};
    int rab = 0, p = 0;
    scanf("%s", s);
    while (s[2] == ' ' || s[2] < 31 || s[2] > 127){
        s[2] = s[1];
        s[1] = s[0];
        s[0] = ' ';
    }
    for (int i = 32; i < 127; i++){
        for (int j = 32; j < 127; j++){
            for (int k = 33; k < 127; k++){
                p++;
                if (s[0] == (char)i && s[1] == (char)j && s[2] == (char)k){
                    rab = 1;
                    if (i > 32) {
                        printf("found: %c%c%c\n", (char)i, (char)j, (char)k);
                    }
                    else if (j > 32) {
                        printf("found: %c%c\n", (char)j, (char)k);
                    }
                    else {
                        printf("found: %c\n", (char)k);
                    }
                    printf("Numbers of attempts: %d\n", p);
                    break;
                }
            }
            if (rab == 1){
                break;
            }
        }
        if (rab == 1){
            break;
        }
    }
    return 0;
}
