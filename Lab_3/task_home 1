#include <stdio.h>
#include <string.h>


int main(void) {
    int n, k = 1;
    scanf("%d", &n);
    for (int i = 0; i < n; i++){
        for (int j = 0; j < n; j++){
            if (j < n - i - 1){
                printf(" ");
            }
            else{
                printf("%d", k);
                k++;
            }
        }
        printf("\n");
    }
    return 0;
}
