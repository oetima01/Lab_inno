package Lab6;
import java.util.Scanner;

public class Main{
    public static void main (String args[]){
        Scanner scan = new Scanner(System.in);
        System.out.print("Enter fahrenheit degree: ");
        float n = scan.nextFloat();
        n = (n - 32) * 5 / 9;
        System.out.printf("Celsius degree: %.2f", n);
    }
}
